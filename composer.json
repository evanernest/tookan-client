{
    "name": "obiefy/tookan-client",
    "description": "Tookan API client for PHP",
    "authors": [
        {
            "name": "Obiefy",
            "email": "obayhamed@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "guzzle/guzzle": "^3.9",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Obiefy\\Tookan\\" : "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Obiefy\\Tookan\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obiefy\\Tookan\\TookanServiceProvider"
            ]
        }
    }
}
